<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for item in wp.get({posts_per_page: -1, orderby: 'date', has_password: 0, post_type: 'any'}) %}
{% if item.post_name != 'sitemap' %}
<url>
	<loc>{{ item.link }}</loc>
</url>
{% endif %}
{% endfor %}
</urlset>